{
  "name": "react-track",
  "version": "4.0.0",
  "description": "A react carousel/slider like component for sequentially displaying slides or sets of slides",
  "main": "lib/track/index.js",
  "scripts": {
    "build": "npm test && NODE_ENV=production babel src --out-dir lib",
    "dev": "webpack-dev-server --quiet --no-info --hot --inline --history-api-fallback --config ./webpack.config.babel.js",
    "preversion": "npm run build && git add ./lib",
    "test": "tape -r babel-register tests/**/*.test.js | tap-spec"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jane/react-track.git"
  },
  "author": "Cory Brown <cory@jane.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jane/react-track/issues"
  },
  "homepage": "https://github.com/jane/react-track#readme",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.7",
    "babel-plugin-react-require": "^3.0.0",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-export-extensions": "^6.8.0",
    "babel-preset-babili": "0.0.8",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-3": "^6.17.0",
    "babel-register": "^6.18.0",
    "css-loader": "^0.26.0",
    "enzyme": "^2.6.0",
    "eslint": "^3.10.2",
    "eslint-plugin-jsx-a11y": "^3.0.1",
    "eslint-plugin-react": "^6.7.1",
    "find-cache-dir": "^0.1.1",
    "jsdom": "^9.8.3",
    "react": "^15.4.0",
    "react-addons-test-utils": "^15.4.1",
    "react-dom": "^15.4.1",
    "style-loader": "^0.13.1",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.3",
    "webpack": "^2.1.0-beta.27",
    "webpack-dev-server": "^2.1.0-beta.11"
  },
  "dependencies": {
    "classnames": "^2.2.5"
  },
  "peerDependencies": {
    "react": "^15.4.0"
  }
